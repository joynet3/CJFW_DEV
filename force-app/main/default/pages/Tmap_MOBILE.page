                            ["msgStartDesc", msgStartDesc],
                            ["msgEndDesc", msgEndDesc],
                            ["msgViaPointsDesc", msgViaPointsDesc]
                        ]);
                        
                        if (typeof startRoadAddr != 'undefined' && typeof marker_s != 'undefined') {
                            msgStart = startRoadAddr;
                        }
                        if (typeof endRoadAddr != 'undefined' && typeof marker_e != 'undefined') {
                            msgEnd = endRoadAddr;
                        }
                        if (typeof totalDistance != 'undefined') {
                            msgDist = totalDistance;
                        }
                        if (typeof select_viaList.length != 0) {
                            for (var i=0; i<select_viaList.length; i++) {
                                msgViaPoints.push(select_viaList[i].roadAddr);
                            }
                        }

                        if (msgStart === '' || msgEnd === '' || msgDist === '') {
                            alert("출발지와 도착지 선택 후 거리 계산하시기 바랍니다.")
                        } else if (ok === 'not ok') {
                            alert("목적지 목록이 변경되었습니다. 거리 계산 후 다시 시도하시기 바랍니다.")
                        } else if (count != msgViaPoints.length) {
                            alert("경유지 선택 후 거리 계산하시기 바랍니다.")
                        } else {
                            msgObj.set("msgStart", msgStart);
                            msgObj.set("msgEnd", msgEnd);
                            msgObj.set("msgDist", msgDist);
                            
                            msgObj.set("msgViaPoints", msgViaPoints);
                            msgObj.set("msgStartDesc", startDescVal);
                            msgObj.set("msgEndDesc", EndDescVal);
                            msgObj.set("msgViaPointsDesc", via_descVal);


                            sendMsgToParent(msgObj);
                            $('div#listContainer').hide();
                            $('div#currentPosBtnContainer').css('z-index', '9999999999999999');
                        }
                    } else if (e.data.get('type') === "CANCEL") {
                        var msgObj = new Map([
                            ["type", "CANCEL"],
                            ["msgStart", msgStart],
                            ["msgEnd", msgEnd],
                            ["msgDist", msgDist],
                            ["msgViaPoints", msgViaPoints]
                        ]);
                        sendMsgToParent(msgObj);
                        $('div#listContainer').hide();
                        $('div#currentPosBtnContainer').css('z-index', '9999999999999999');
                    } else {

                        if (ok === 'ok') {
                            map.setCenter(new Tmapv2.LatLng(e.data.get('msgLat'),e.data.get('msgLng')));
                            map.setZoom(16);
                        } else {
                            var msgObj = new Map([
                                ["type", "IN PROCESS"],
                                ["msgStart", ''],
                                ["msgEnd", ''],
                                ["msgDist", ''],
                                ["msgViaPoints", []]
                            ]);

                            sendMsgToParent(msgObj);

                            console.log('msgLat:::::::::' + e.data.get('msgLat'));
                            console.log('msgLng:::::::::' + e.data.get('msgLng'));

                            map.setCenter(new Tmapv2.LatLng(e.data.get('msgLat'),e.data.get('msgLng')));

                            let latlng = {lat: e.data.get('msgLat'), lng: e.data.get('msgLng')};
                            console.log('request-string:::::::::::::::' + JSON.stringify(latlng));

                            current_latlng = new Tmapv2.LatLng(e.data.get('msgLat'), e.data.get('msgLng'));
                            current_lat = e.data.get('msgLat').toString();
                            current_lng = e.data.get('msgLng').toString();

                            fnGetReverseGeocoding(JSON.stringify(latlng));
                        }

                    }

                };

                $('#currentPosBtn').click(function() {
                    $('div#startEndList').hide();
                    totalDistance = undefinedVar;
                    
                    var msgObj = new Map([
                            ["type", "GET CURRENT LOCATION"],
                            ["msgStart", ''],
                            ["msgEnd", ''],
                            ["msgDist", ''],
                            ["msgViaPoints", []]
                        ]);

                        sendMsgToParent(msgObj);
                });

                // 현재 위치 주소로 검색
                function fnSearchCurrentAddr(currentAddr) {
                    console.log('currentAddr:::::::::' + currentAddr);

                    console.log('active::::::::::::::' + activeElement);

                    let req;
                    if (activeElement === 'start_input') {
                        req = {type: 'current-start', search: currentAddr};
                    } else if (activeElement === 'via_input_1') {
                        req = {type: 'current-via', search: currentAddr, index: '1'};
                    } else if (activeElement === 'via_input_2') {
                        req = {type: 'current-via', search: currentAddr, index: '2'};
                    } else if (activeElement === 'via_input_3') {
                        req = {type: 'current-via', search: currentAddr, index: '3'};
                    } else if (activeElement === 'via_input_4') {
                        req = {type: 'current-via', search: currentAddr, index: '4'};
                    } else if (activeElement === 'via_input_5') {
                        req = {type: 'current-via', search: currentAddr, index: '5'};
                    } else if (activeElement === 'via_input_6') {
                        req = {type: 'current-via', search: currentAddr, index: '6'};
                    } else if (activeElement === 'via_input_7') {
                        req = {type: 'current-via', search: currentAddr, index: '7'};
                    } else if (activeElement === 'via_input_8') {
                        req = {type: 'current-via', search: currentAddr, index: '8'};
                    } else if (activeElement === 'via_input_9') {
                        req = {type: 'current-via', search: currentAddr, index: '9'};
                    } else {
                        req = {type: 'current-end', search: currentAddr};
                    }

                    fnSearchPlace(JSON.stringify(req));
                }

                // 부모에게 메시지 전달
                function sendMsgToParent( msg ) {
                    window.parent.postMessage( msg, '*' );
                }

            </script>
        </body>
