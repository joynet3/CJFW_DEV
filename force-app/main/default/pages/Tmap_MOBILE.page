                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!if(tmapItemResults.size == 0, true, false)}">
                                        <li>
                                            <span class="slds-assistive-text" aria-live="polite">
                                                ""에 대한 검색결과가 존재하지 않습니다.
                                            </span>
                                        </li>
                                    </apex:outputPanel>
                                </apex:repeat>
                                <script type="text/javascript">
                                    let tempList = '';
                                    if ('{!jsonResultList}' != '') {
                                        tempList = jQuery.parseJSON('{!JSENCODE(jsonResultList)}');

                                        var msgObj = new Map([
                                            ["type", "SUCCESS"],
                                            ["msgStart", ''],
                                            ["msgEnd", ''],
                                            ["msgDist", ''],
                                            ["msgViaPoints", []]
                                        ]);

                                        sendMsgToParent(msgObj);
                                    }
                                    let listButton = document.querySelectorAll('#listButton');
                                    let type;
                                    let name_se;
                                    let src_se;
                                    let char_se;
                                    let num_se;
                                    let roadAddr_se;
                                    let noorLat_se;
                                    let noorLon_se;
                                    let type_se;
                                    let newMapHeight = $('#map_div').height();

                                    if ('{!tmapItemResults.size}' == 0) {
                                        console.log('Empty List::::::::::::::::::::');

                                        $('div#listContainer').hide();
                                        // $('div#listContainer').show();
                                        $("#toggleListBtn").hide();
                                        // $('div#currentPosBtnContainer').css('z-index', '9999999999999999');

                                        $('div#noResultContainer').show();

                                        var searchKeyword = $('#'+document.activeElement.id).val();
                                        var keywordSpan = document.getElementById('searchKeyword');
                                        keywordSpan.innerHTML = '';
                                        keywordSpan.innerHTML += '"' + searchKeyword + '"';
                                        var msg = document.getElementById('noResultMsg');
                                        msg.innerHTML = '';
                                        msg.innerHTML += '에 대한 검색 결과가 없습니다.';

                                        fnChangeMapSize(newMapHeight);
                                        
                                    } else {
                                        console.log('Full List::::::::::::::::::::');
                                        $('div#listContainer').show();
                                        $("#toggleListBtn").show();
                                        $('div#currentPosBtnContainer').css('z-index', '1');

                                        $('div#noResultContainer').hide();
                                        fnChangeMapSize(newMapHeight);

                                        if (!tempList[0].type.includes('current')) {
                                            // 리스트 버튼 이벤트(hoverIn)
                                            listButton.forEach(item => {
                                                item.addEventListener('mouseenter', handleListHoverInEvent);
                                            });

                                            // 리스트 버튼 이벤트(hoverIn)
                                            function handleListHoverInEvent(event) {
                                                // console.log('src:::::::::::::::::' + event.target.querySelector('#img').src);
                                                if (!event.target.querySelector('#img').src.includes('_b_') && !event.target.querySelector('#img').src.includes('_r_')) {
                                                    event.target.querySelector('#img').src = event.target.querySelector('#img').src.replace(/_g_/, '_w_');
                                                    event.target.querySelector('#addr_name').style.color = 'rgba(157, 62, 23, 1)';
                                                }
                                            }

                                            // 리스트 버튼 이벤트(hoverOut)
                                            listButton.forEach(item => {
                                                item.addEventListener('mouseleave', handleListHoverOutEvent);
                                            });

                                            // 리스트 버튼 이벤트(hoverOut)
                                            function handleListHoverOutEvent(event) {
                                                // console.log('src:::::::::::::::::' + event.target.querySelector('#img').src);
                                                if (!event.target.querySelector('#img').src.includes('_b_') && !event.target.querySelector('#img').src.includes('_r_')){
                                                    event.target.querySelector('#img').src = event.target.querySelector('#img').src.replace(/_w_/, '_g_');
                                                    event.target.querySelector('#addr_name').style.color = 'black';
                                                }
                                            }
                                        }
                                    }

                                    if (tempList[0].type.includes('current')) {
                                        if (tempList[0].type === 'current-start') {
                                            $('#start_input').val(tempList[0].roadAddr);
                                            listButton[0].querySelector('#img').src = listButton[0].querySelector('#img').src.replace(/_g_m_./, '_b_m_s');
                                            listButton[0].querySelector('#addr_name').style.color = 'rgba(157, 62, 23, 1)';

                                            src_se = listButton[0].querySelector('#img').src;
                                            name_se = tempList[0].name;
                                            char_se = tempList[0].chr;
                                            num_se = char_se.charCodeAt(0) - 97;
                                            roadAddr_se = tempList[0].roadAddr;
                                            noorLat_se = tempList[0].noorLat;
                                            noorLon_se = tempList[0].noorLon;
                                            type_se = tempList[0].type;
