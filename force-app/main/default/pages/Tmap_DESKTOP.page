                function fnAddToViaList(num, item) {
                    select_viaList[num-1] = item;
                }

                function debouncing(func, timeout = 300) {
                    clearTimeout(timerId);
                    timerId = setTimeout(func, timeout);
                }

                let timer;

                // 경유지 검색
                function fnViaSearchPlace(evtObj, input_id) {
                    
                    $(document).on("keyup",'#'+input_id,function(e) {
                        if ( timer){
                            clearTimeout(timer);
                        } 
                        timer = setTimeout(function() {
                            let keyword = $('#'+input_id).val();
                            let via_id_num = input_id.charAt(input_id.length - 1);
                            console.log('경유지 ' + via_id_num + ' : '+ keyword);

                            if ( oldViaInputList[via_id_num] == $('#'+input_id).val()){
                                return; 
                            }

                            let req = {type: 'via', search: $('#'+input_id).val(), index: via_id_num};

                            if (via_markerArr.length != 0) {
                                if (via_markerArr[num_se-1] !== undefined) {
                                    via_markerArr[num_se-1].setMap(null);
                                    via_markerArr.splice(num_se-1, 1);
                                }
                                totalDistance = undefinedVar;
                            }

                            //기존 맵에 있던 정보들 초기화
                            resettingMap();
                            $('div#startEndList').hide();
                            $('div#distContainer').hide();
                            oldViaInputList[via_id_num] = keyword;
                            
                            // apex 메소드 호출
                            fnSearchViaPlace(JSON.stringify(req));
                            console.log('fnViaSearchPlace end');
                        }, 500);

                    });

                    
                    // 출발지 키워드 초기화
                    $(document).on("focusout",'#'+input_id,function(e) {
                        if ( timer){
                            clearTimeout(timer);
