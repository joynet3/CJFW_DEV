                                <input type='checkbox' fieldtype="CHECK" disabled="true" checked="checked"/>&nbsp;<label>전기</label>
                            </apex:outputtext>
                            <apex:outputtext rendered="{!!IF(CONTAINS(objOpportunity.Proposal_Utility__c,'전기'), true, false)}">
                                <input type='checkbox' fieldtype="CHECK" disabled="true"/>&nbsp;<label>전기</label>
                            </apex:outputtext>
                            &nbsp;&nbsp;
                            <apex:outputtext rendered="{!IF(CONTAINS(objOpportunity.Proposal_Utility__c,'수도'), true, false)}">
