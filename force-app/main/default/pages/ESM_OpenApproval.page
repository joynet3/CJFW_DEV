                            <apex:outputtext rendered="{!IF(CONTAINS(objOpportunity.Proposal_Meal__c,'상수도'), true, false)}">
                                <input type='checkbox' fieldtype="CHECK" disabled="true" checked="checked"/>&nbsp;<label>상수도</label>
                            </apex:outputtext>
                            <apex:outputtext rendered="{!!IF(CONTAINS(objOpportunity.Proposal_Meal__c,'상수도'), true, false)}">
                                <input type='checkbox' fieldtype="CHECK" disabled="true"/>&nbsp;<label>상수도</label>
                            </apex:outputtext>
                            &nbsp;&nbsp;
                            <apex:outputtext rendered="{!IF(CONTAINS(objOpportunity.Proposal_Meal__c,'지하수'), true, false)}">
                                <input type='checkbox' fieldtype="CHECK" disabled="true" checked="checked"/>&nbsp;<label>지하수</label>
                            </apex:outputtext>
                            <apex:outputtext rendered="{!!IF(CONTAINS(objOpportunity.Proposal_Meal__c,'지하수'), true, false)}">
                                <input type='checkbox' fieldtype="CHECK" disabled="true"/>&nbsp;<label>지하수</label>
                            </apex:outputtext>
